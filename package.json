{
  "name": "authentication-api",
  "version": "1.0.0",
  "description": "A boilerplate for the nodejs authentication",
  "main": "src/server.js",
  "scripts": {
    "dev": "babel-watch src/server.js",
    "db:create": "node_modules/.bin/sequelize db:create",
    "db:migrate": "node_modules/.bin/sequelize db:migrate",
    "db:migrate:reset": "node_modules/.bin/sequelize db:migrate:undo:all",
    "db:seed": "node_modules/.bin/sequelize db:seed:all",
    "db:seed:undo": "node_modules/.bin/sequelize db:seed:undo",
    "db:seed:reset": "node_modules/.bin/sequelize db:seed:undo:all"
  },
  "author": "Carine SEMWAGA",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "celebrate": "^15.0.1",
    "dotenv": "^16.0.2",
    "express": "^4.18.1",
    "jsonwebtoken": "^8.5.1",
    "moment": "^2.29.4",
    "pg": "^8.8.0",
    "pg-hstore": "^2.3.4",
    "sequelize": "^6.22.1"
  },
  "devDependencies": {
    "@babel/preset-env": "^7.19.1",
    "babel-polyfill": "^6.26.0",
    "babel-watch": "^7.7.0",
    "sequelize-cli": "^6.4.1"
  }
}
